<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joker.blurapplication">

  <uses-permission android:name="android.permission.INTERNET"/>

  <application
      android:name=".MyApplication"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme.WhiteActivity">

    <meta-data android:name="com.joker.blurapplication.GlideConfiguration"
        android:value="GlideModule"/>

    <activity android:name=".activity.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity android:name=".activity.bluractivity.RenderScriptNavigateActivity"
        android:theme="@style/AppTheme.WhiteActivity"
        />

    <activity android:name=".activity.bluractivity.renderscript.PicassoBlurActivity"
        android:theme="@style/AppTheme.WhiteActivity"
        />

    <activity android:name=".activity.bluractivity.renderscript.GlideBlurActivity"
        android:theme="@style/AppTheme.WhiteActivity"
        />

    <activity android:name=".activity.bluractivity.renderscript.RemoteBlurActivity"
        android:theme="@style/AppTheme.WhiteActivity"
        />

    <activity android:name=".activity.bluractivity.FastBlurActivity"
        android:theme="@style/AppTheme.WhiteActivity"
        />

    <activity android:name=".activity.bluractivity.PaintBlurActivity"
        android:theme="@style/AppTheme.WhiteActivity"
        />

    <activity android:name=".activity.bluractivity.RxAnimatorBlurActivity"
        android:theme="@style/AppTheme.WhiteActivity"
        />
  </application>

</manifest>
